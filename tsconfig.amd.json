{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "noEmit": false,
    "noEmitHelpers": false,
    /*                   */
    "module": "amd",
    "outFile": "./build/js/questrade.amd.js",
    "tsBuildInfoFile": "./build/tmp/tsbuild.amd.info",
    "importHelpers": true,
    "baseUrl": "./",
    "paths": {
      "tslib": ["node_modules/tslib/tslib.d.ts"]
    },
    /* Additional Vebosity */
    "diagnostics": true,
    "extendedDiagnostics": false,
    "listFiles": false,
    "listEmittedFiles": false,
    "traceResolution": false,
    /* Source Map Options */
    "composite": false,
    "declaration": false,
    "declarationMap": false,
    "sourceMap": false,
    "inlineSourceMap": false,
    "inlineSources": false
  }
}
