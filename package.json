{
  "name": "questrade-ts",
  "displayName": "Questrade API by Luxcium (with typescript support).",
  "description": "Unofficial Questrade API wrapper for NodeJS with full TypeScript support.",
  "version": "1.0.0-rc7",
  "author": "Luxcium <luxcium@neb401.com>",
  "license": "MIT",
  "homepage": "https://github.com/Luxcium/questrade-ts",
  "main": "./src/index.js",
  "types": "./typings/index.d.ts",
  "scripts": {
    "prebuild": "yarn run test",
    "build": "tsc -b ./configs/tsconfig.commonjs.json",
    "pretest": "yarn run linting",
    "test": "echo 'will skip test now'",
    "posttest": "#jest --coverage",
    "linting": "yarn run prettier && yarn run tslint && yarn run tsc",
    "prettier": "prettier --check --write --config .prettierrc './src/**/*.ts'",
    "tslint": "tslint --project ./ --format codeFrame --fix",
    "tsc": "tsc --noEmit --pretty",
    "predist": "yarn run build:clean; yarn run rm:dist",
    "dist": "yarn run cp:dist",
    "postdist": "npm publish ./dist --tag latest --dry-run && npm pack ./dist --tag latest",
    "rm:dist": "rimraf ./dist/",
    "cp:dist": "concurrently \"yarn:cp:dist:1\" \"yarn:cp:dist:2\" \"yarn:cp:dist:3\" \"yarn:cp:dist:4\" \"yarn:cp:dist:5\"",
    "cp:dist:1": "cp-cli ./package.1.json ./dist/package.json",
    "cp:dist:2": "cp-cli ./readme.md ./dist/readme.md",
    "cp:dist:3": "cp-cli ./LICENSE ./dist/LICENSE",
    "cp:dist:4": "cp-cli ./build/src ./dist/src",
    "cp:dist:5": "cp-cli ./build/typings ./dist/typings",
    "prebuild:clean": "rimraf -r ./build/**/*",
    "build:clean": "yarn run build",
    "latest:yarn": "yarn add axios@latest questrade-api-enumerations@latest tslib@latest http-status-codes@latest source-map-support@latest",
    "out:cjs": "tsc -b ./configs/tsconfig.commonjs.json",
    "out:umd": "tsc -b ./configs/tsconfig.umd.json",
    "out:amd": "tsc -b ./configs/tsconfig.amd.json",
    "out:sys": "tsc -b ./configs/tsconfig.sys.json",
    "quick:test": "jest",
    "out:enu": "tsc -b ./enums/tsconfig.json && yarn run cp:enu:yarn",
    "npm:enum:publish": " npm publish ./build/enums/",
    "cp:enu": "concurrently \"yarn:cp:enu:1\" \"yarn:cp:enu:2\" \"yarn:cp:enu:3\" \"yarn:cp:enu:4\"",
    "cp:enu:1": "cp-cli ./enums/README.md ./build/enums/README.md",
    "cp:enu:2": "cp-cli ./enums/package.json ./build/enums/package.json",
    "cp:enu:3": "cp-cli ./enums/.npmignore ./build/enums/.npmignore",
    "cp:enu:4": "cp-cli ./enums/LICENSE.txt ./build/enums/LICENSE.txt"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com/luxcium/questrade-ts.git"
  },
  "bugs": {
    "url": "https://github.com/luxcium/questrade-ts/issues"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "http-status-codes": "^1.3.2",
    "questrade-api-enumerations": "^1.1.4",
    "source-map-support": "^0.5.13",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/parser": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-typescript": "^7.6.0",
    "@babel/types": "^7.6.1",
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/jasmine": "^3.4.1",
    "@types/jest": "^24.0.18",
    "@types/lodash": "^4.14.141",
    "@types/node": "12.6.8",
    "@types/ramda": "^0.26.26",
    "@types/sanctuary": "^0.14.2",
    "babel-jest": "^24.9.0",
    "chai": "^4.2.0",
    "concurrently": "4.1.2",
    "coveralls": "^3.0.6",
    "cp-cli": "2.0.0",
    "jest": "^24.9.0",
    "lodash": "^4.17.15",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "ramda": "^0.26.1",
    "rimraf": "2.6.3",
    "sanctuary": "^2.0.0",
    "ts-jest": "^24.1.0",
    "ts-node": "8.3.0",
    "tslint": "^5.18.0",
    "tslint-config-airbnb": "5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-security": "^1.16.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.5.3",
    "typescript-tslint-plugin": "0.5.4"
  },
  "keywords": [
    "Questrade",
    "stocks",
    "trading",
    "typescript",
    "Unofficial",
    "Questrade API",
    "API wrapper",
    "TypeScript support",
    "stock market api",
    "canada",
    "Canadian"
  ]
}
