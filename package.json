{
  "name": "questrade-ts",
  "displayName": "Questrade API by Luxcium (with typescript support).",
  "description": "Unofficial Questrade API wrapper for NodeJS with full TypeScript support.",
  "version": "1.0.0-3rc-UNSAFE",
  "author": "Luxcium <luxcium@neb401.com>",
  "license": "MIT",
  "homepage": "https://github.com/Luxcium/questrade-ts",
  "main": "./src/api/index.js",
  "types": "./lib/api/index.d.ts",
  "scripts": {
    "predist": "yarn run build",
    "dist": "yarn run rm:dist; yarn run cp:dist",
    "postdist": "npm publish ./dist --tag next --dry-run; npm pack ./dist",
    "rm:dist": "rimraf ./dist/src/; rimraf ./dist/lib/; ",
    "cp:dist": "concurrently \"yarn:cp:dist:0\" \"yarn:cp:dist:1\" \"yarn:cp:dist:2\" \"yarn:cp:dist:3\"",
    "cp:dist:0": "cp-cli ./build/src ./dist/src; cp-cli ./build/lib ./dist/lib",
    "cp:dist:1": "cp-cli ./package.1.json ./dist/package.json",
    "cp:dist:2": "cp-cli ./readme.md ./dist/readme.md",
    "cp:dist:3": "cp-cli ./LICENSE ./dist/LICENSE",
    "prebuild": "yarn run out:pre:clean",
    "build": "yarn run quick",
    "out:pre:clean": "rimraf -r ./build/**/*",
    "out:post:clean": "yarn run rm:typescript; yarn run rm:test",
    "rm:test": "rimraf ./build/src/test; rimraf ./build/lib/test rmraf ./build/js/umd/src/test; rimraf ./build/js/umd/lib/test;",
    "rm:typescript": "rimraf ./build/src/api/core/typescript/; rimraf ./build/js/umd/src/api/core/typescript/",
    "prequick": "yarn run test",
    "quick": "yarn run rebuild",
    "rebuild": "concurrently \"yarn:out:cjs\" \"yarn:out:umd\" \"yarn:out:amd\" \"yarn:out:sys\"",
    "postrebuild": "yarn run out:post:clean",
    "out:cjs": "tsc -b ./tsconfig.commonjs.json",
    "out:umd": "tsc -b ./tsconfig.umd.json",
    "out:amd": "tsc -b ./tsconfig.amd.json",
    "out:sys": "tsc -b ./tsconfig.sys.json",
    "lint": "yarn run prettier && yarn run tslint",
    "tslint": "tslint --project . --format codeFrame --fix",
    "pretest": "yarn run lint && tsc --noEmit --pretty",
    "prettier": "prettier --check --write --config .prettierrc src/* src/api/* src/api/core/* src/api/core/class/* src/api/core/typescript/* src/api/utils/*",
    "test": "jest",
    "latest": "npm i axios@latest questrade-api-enumerations@latest tslib@latest http-status-codes@latest",
    "out:enu": "tsc -b ./enums/tsconfig.json && yarn run cp:enu:yarn",
    "npm:enum:publish": " npm publish ./build/enums/",
    "cp:enu": "concurrently \"yarn:cp:enu:0\" \"yarn:cp:enu:1\" \"yarn:cp:enu:2\" \"yarn:cp:enu:3\"",
    "cp:enu:0": "cp-cli ./enums/LICENSE.txt ./build/enums/LICENSE.txt",
    "cp:enu:1": "cp-cli ./enums/README.md ./build/enums/README.md",
    "cp:enu:2": "cp-cli ./enums/package.json ./build/enums/package.json",
    "cp:enu:3": "cp-cli ./enums/.npmignore ./build/enums/.npmignore"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com/luxcium/questrade-ts.git"
  },
  "bugs": {
    "url": "https://github.com/luxcium/questrade-ts/issues"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "http-status-codes": "^1.3.2",
    "questrade-api-enumerations": "^1.1.4",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/parser": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-typescript": "^7.6.0",
    "@babel/types": "^7.6.1",
    "@types/jasmine": "^3.4.1",
    "@types/jest": "^24.0.18",
    "@types/node": "12.6.8",
    "babel-jest": "^24.9.0",
    "concurrently": "4.1.2",
    "coveralls": "^3.0.6",
    "cp-cli": "2.0.0",
    "jest": "^24.9.0",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "rimraf": "2.6.3",
    "ts-jest": "^24.1.0",
    "ts-node": "8.3.0",
    "tslint": "^5.18.0",
    "tslint-config-airbnb": "5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-security": "^1.16.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.5.3",
    "typescript-tslint-plugin": "0.5.4"
  },
  "keywords": [
    "Questrade",
    "stocks",
    "trading",
    "typescript",
    "Unofficial",
    "Questrade API",
    "API wrapper",
    "TypeScript support",
    "stock market api",
    "canada",
    "canadian"
  ]
}
