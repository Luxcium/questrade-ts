{
  "name": "questrade-ts",
  "displayName": "Questrade API by Luxcium (with typescript support).",
  "description": "Unofficial Questrade API wrapper for NodeJS with full TypeScript support.",
  "version": "2.0.0-beta",
  "private": false,
  "author": "Luxcium <luxcium@neb401.com>",
  "license": "MIT",
  "homepage": "https://github.com/Luxcium/questrade-ts",
  "main": "./src/index.js",
  "types": "./typings/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git@github.com/luxcium/questrade-ts.git"
  },
  "bugs": {
    "url": "https://github.com/luxcium/questrade-ts/issues"
  },
  "keywords": [
    "TypeScript",
    "Canadian",
    "Stock Market API",
    "Un-Official",
    "Questrade",
    "Canada",
    "API Wrapper",
    "Questrade API",
    "Stocks",
    "Trading",
    "Luxcium",
    "TypeScript Support",
    "API wrapper",
    "TypeScript support",
    "stock market api",
    "canada",
    "Canadian"
  ],
  "scripts": {
    "start": "ts-node './src/main.ts'",
    "build": "tsc --build configs/tsconfig.build.json --pretty || exit 1",
    "preinstall": "rm -fr node_modules; echo ''; pwd; ls -1A; echo ''",
    "postinstall:old": "(cd 'src/resources/' && rm -fr ./node_modules && echo '' && pwd && ls -1A && echo '' && (npm ci || npm install) && echo -e '\n― OK ―\n'  )",
    "pretty": "prettier --config \"./.prettierrc\" --loglevel log -c \"./src/**/*.ts\"|| exit 0",
    "pretty:fix": "prettier --config \"./.prettierrc\" --loglevel log -c --write \"./src/**/*.ts\" || exit 1",
    "lint": "eslint \"./src/**/*.ts\" --format=compact --fix-dry-run || exit 0",
    "lint:fix": "eslint \"./src/**/*.ts\" --format=stylish --fix || exit 1",
    "tsc:noemit": "tsc --noEmit --pretty || exit 1",
    "prelinting": "yarn run pretty:fix || exit 3",
    "linting": "yarn run lint:fix || exit 3",
    "postlinting": "yarn run tsc:noemit || exit 3",
    "pretest": "yarn run linting",
    "test": "echo 'will skip test now'",
    "posttest": "# yarn run quick:test",
    "SECTION:001": "# ========================================================",
    "predist": "yarn run build; yarn run rm:dist",
    "dist": "yarn run rm:playground; yarn run cp:dist",
    "postdist": "npm publish ./dist --dry-run --tag beta  && npm pack ./dist --tag beta",
    "rm": "# =================================================================",
    "rm:build": "rimraf -r ./build/**/*",
    "rm:dist": "rimraf ./dist/",
    "rm:playground": "rimraf ./build/src/test/playground",
    "rm:pack1": "rimraf ./package.1.json",
    "cp": "# =================================================================",
    "cp:dist": "concurrently -n \"README|LICENSE|DIST_SRC|DIST_TYPINGS\" --name-separator \"|\" \"yarn:cp:dist:1\" \"yarn:cp:dist:2\" \"yarn:cp:dist:3\" \"yarn:cp:dist:4\" \"yarn:cp:dist:5\" && yarn cp:pack && cd dist && npm install",
    "postcp:dist": "#npx parcel build --no-cache --no-source-maps --experimental-scope-hoisting ./dist/src/index.js;rimraf ./dist/node_modules; rimraf ./build ;#rimraf ./dist/src",
    "cp:dist:1": "cpr ./README.md ./dist/ -o",
    "cp:dist:2": "cpr \"./.npmignore\" \"./dist/.npmignore\" -o",
    "cp:dist:3": "cpr LICENSE ./dist/ -o",
    "cp:dist:4": "cpr \"./build/src\" \"./dist/src\" -d",
    "cp:dist:5": "cpr \"./build/typings\" \"./dist/typings\" -d",
    "cp:pack": "node ./scripts/pre-publish.js",
    "sum": "./scripts/sums.sh"
  },
  "devDependencies": {
    "@babel/core": "7.12.16",
    "@babel/preset-env": "7.12.16",
    "@types/eslint-config-prettier": "6.11.0",
    "@types/eslint-plugin-prettier": "3.1.0",
    "@types/jest": "26.0.20",
    "@types/ioredis": "4.19.2",
    "@types/node": "14.14.28",
    "@types/ramda": "0.27.38",
    "@types/redis": "2.8.28",
    "@typescript-eslint/eslint-plugin": "4.15.1",
    "@typescript-eslint/parser": "4.15.1",
    "babel-jest": "26.6.3",
    "concurrently": "5.3.0",
    "cpr": "3.0.1",
    "eslint": "7.20.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-formatter-github": "1.0.11",
    "eslint-import-resolver-node": "0.3.4",
    "eslint-plugin-fp": "2.3.0",
    "eslint-plugin-functional": "3.2.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "32.0.1",
    "eslint-plugin-jsonc": "0.9.0",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-promise": "4.3.1",
    "eslint-plugin-radar": "0.2.1",
    "eslint-plugin-security": "1.4.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-sort-keys-fix": "1.1.1",
    "eslint-plugin-tsdoc": "0.2.11",
    "eslint-plugin-unicorn": "28.0.1",
    "jest": "26.6.3",
    "prettier": "2.2.1",
    "questrade-api-enumerations": "1.1.4",
    "rimraf": "3.0.2",
    "snyk": "1.455.0",
    "ts-jest": "26.5.1",
    "ts-node": "9.1.1",
    "tslib": "2.1.0",
    "typescript": "~4.1.5"
  },
  "dependencies": {
    "axios": "0.21.1",
    "dotenv": "8.2.0",
    "ioredis": "4.19.4",
    "redis": "3.0.2",
    "redis-json": "4.3.0",
    "redis-rejson": "1.0.0",
    "tedis": "0.1.12"
  },
  "optionalDependencies": {},
  "bundledDependencies": [
    "questrade-api-enumerations"
  ]
}
