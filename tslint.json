{
  "extends": [
    "tslint:latest",
    "tslint-config-airbnb",
    "tslint-config-security",
    "tslint-plugin-prettier",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "./build",
      "./node_modules/**/*"
    ]
  },
  "defaultSeverity": "warning",
  "rulesDirectory": [],
  "rules": {
    "no-any": true,
    "typedef": [
      false
    ],
    "ordered-imports": false,
    "prettier": false,
    "import-name": true,
    "max-line-length": [
      true,
      {
        "ignore-pattern": "",
        "limit": 80,
        "check-strings": false
      }
    ],
    "no-boolean-literal-compare": true,
    "no-console": false,
    "no-unused-expression": true,
    "object-literal-sort-keys": false,
    "object-shorthand-properties-first": false,
    "triple-equals": [
      true,
      "allow-null-check"
    ],
    "object-literal-shorthand": false,
    "interface-name": false,
    "ter-arrow-parens": [
      true,
      "as-needed",
      {
        "requireForBlockBody": false
      }
    ],
    "promise-function-async": true,
    "function-name": false,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-pascal-case",
      "allow-leading-underscore",
      "allow-trailing-underscore"
    ]
  },
  "jsRules": {
    "prettier": false,
    "import-name": false,
    "max-line-length": [
      true,
      {
        "limit": 80,
        "check-strings": false
      }
    ],
    "no-console": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "triple-equals": [
      true,
      "allow-null-check"
    ],
    "ter-arrow-parens": [
      true,
      "as-needed",
      {
        "requireForBlockBody": false
      }
    ],
    "promise-function-async": true,
    "function-name": false,
    "variable-name": [
      true,
      "allow-leading-underscore",
      "check-format",
      "allow-trailing-underscore",
      "allow-pascal-case",
      "ban-keywords"
    ]
  }
}
