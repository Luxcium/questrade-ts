{
  "compilerOptions": {
    /* Basic Options */
    "noEmit": false,
    "noEmitOnError": false,
    "target": "es6",
    "module": "commonjs",
    "rootDir": "./src",
    "outDir": "./out/src",
    "declarationDir": "./out/typings",
    /* Compilation Options */
    "allowJs": true,
    "checkJs": false,
    "importHelpers": true,
    "newLine": "lf",
    "noEmitHelpers": false,
    "noErrorTruncation": true,
    "removeComments": false,
    "resolveJsonModule": true,
    "skipLibCheck": false,
    "useDefineForClassFields": true,
    "importsNotUsedAsValues": "remove",
    "forceConsistentCasingInFileNames": true,
    "assumeChangesOnlyAffectDirectDependencies": false,
    "generateCpuProfile": "./performance/.cpuprofile",
    /* Incremental compilation. */
    "incremental": true,
    "tsBuildInfoFile": "./out/ts-build-info.lib",
    /* Module Resolution Options */
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "composite": true,
    "isolatedModules": true,
    /* Source Map and declaration options */
    "declaration": true,
    "declarationMap": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "sourceMap": false,
    "emitDeclarationOnly": false,
    /* Vebosity */
    "pretty": true,
    "diagnostics": true,
    "extendedDiagnostics": true,
    "listEmittedFiles": false,
    "listFiles": false,
    "traceResolution": false,
    "listFilesOnly": false,
    /* Extra settings */
    "disableReferencedProjectLoad": false,
    "disableSolutionSearching": false,
    "disableSourceOfProjectReferenceRedirect": true,
    "downlevelIteration": true,
    "emitBOM": false,
    "noResolve": false,
    "noImplicitUseStrict": false,
    "noStrictGenericChecks": false,
    "noUncheckedIndexedAccess": false,
    "preserveConstEnums": false,
    "preserveSymlinks": false,
    "stripInternal": false,
    /* Additional Options */
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    /* List of library files to be included in the compilation. */
    "lib": [
      "dom",
      "esnext"
    ],
    "noLib": false,
    /* list of directories for type definition */
    "typeRoots": [
      "./node_modules/@types",
      "./node_modules/.registry.npmjs.org/@types",
      "./typings"
    ],
    /* Type declaration files to be included in compilation. */
    "types": [
      "jest",
      "node"
    ],
    /* ALWAYS FALSE NEVER ALLOW-UNUSED-LABELS & ALLOW-UNREACHABLE-CODE FALSE */
    /* ALWAYS FALSE */ "allowUmdGlobalAccess": false /*         ALWAYS FALSE */,
    /* ALWAYS FALSE */ "allowUnreachableCode": false /*         ALWAYS FALSE */,
    /* ALWAYS FALSE */ "allowUnusedLabels": false /*            ALWAYS FALSE */,
    /* ALWAYS FALSE NEVER ALLOW-UNUSED-LABELS & ALLOW-UNREACHABLE-CODE FALSE */
    /*                */ "strict": true /*                                   */,
    /* ALWAYS TRUE STRICT TYPE-CHECKING ADDITIONAL CHECKS MUST ALWAYS BE TRUE*/
    /* ALWAYS TRUE  */ "alwaysStrict": true /*                  ALWAYS TRUE  */,
    /* ALWAYS TRUE  */ "noImplicitAny": true /*                 ALWAYS TRUE  */,
    /* ALWAYS TRUE  */ "noImplicitThis": true /*                ALWAYS TRUE  */,
    /* ALWAYS TRUE  */ "strictBindCallApply": true /*           ALWAYS TRUE  */,
    /* ALWAYS TRUE  */ "strictFunctionTypes": true /*           ALWAYS TRUE  */,
    /* ALWAYS TRUE  */ "strictNullChecks": true /*              ALWAYS TRUE  */,
    /* ALWAYS TRUE  */ "strictPropertyInitialization": true /*  ALWAYS TRUE  */,
    /* ALWAYS TRUE  */ "noFallthroughCasesInSwitch": true /*    ALWAYS TRUE  */,
    /* ALWAYS TRUE  */ "noImplicitReturns": true /*             ALWAYS TRUE  */,
    /* ALWAYS TRUE  */ "noUnusedLocals": true /*                ALWAYS TRUE  */,
    /* ALWAYS TRUE  */ "noUnusedParameters": true /*            ALWAYS TRUE  */,
    /* ALWAYS TRUE STRICT TYPE-CHECKING ADDITIONAL CHECKS MUST ALWAYS BE TRUE*/
    /*                                                                        */
    "paths": {
      "~*": [
        "./*"
      ]
    }
    /* Unused settings */
    /*                                                                        */
    // "watch": false,
    // "baseUrl": "./",
    // "charset": "",
    // "disableSizeLimit": false,
    // "fallbackPolling": "dynamicPriorityPolling",
    // "jsx": "preserve",
    // "jsxFactory": "React.createElement",
    // "jsxFragmentFactory": "React.Fragment",
    // "jsxImportSource": "react",
    // "keyofStringsOnly": false,
    // "mapRoot": "",
    // "maxNodeModuleJsDepth": 0,
    // "outFile": "",
    // "plugins": [],
    // "preserveWatchOutput": true,
    // "reactNamespace": "React",
    // "rootDirs": [],
    // "skipDefaultLibCheck": false,
    // "sourceRoot": "",
    // "suppressExcessPropertyErrors": false,
    // "suppressImplicitAnyIndexErrors": false,
    // "watchDirectory": "dynamicPriorityPolling",
    // "watchFile": "dynamicPriorityPolling",
    /*                                                                        */
  },
  "include": [
    "./src/**/*.ts",
  ],
  "exclude": [
    ".git",
    ".github",
    ".vscode",
    "**/__test__/**/*.*",
    "**/node_modules",
    "backup",
    "build",
    "configs",
    "debug.js",
    "enums",
    "functional.js",
    "keys",
    "manualTestingAndDebug.js",
    "misc",
    "out",
    "scripts",
  ],
}
